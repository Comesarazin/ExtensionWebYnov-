{
    "manifest_version":3,
    "name": "MyTodo",
    "version": "1.00",
    "description": "My Todo listes",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],

    "action": {
        "default_icon": {

            "32": "images/logoTodo.jpg",
            "64": "images/logoTodo.jpg"
            
        },
        "default_popup": "views/listes.html"
    },
    "icons": {
        "64": "images/logoTodo.jpg"
    }
}